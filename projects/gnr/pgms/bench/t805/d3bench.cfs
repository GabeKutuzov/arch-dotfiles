T805 (memory=4M) master;


connect master.link[1] to host;

input from_server;
output to_server;

process (stacksize = 8K, heapsize = 2000K,
         interface(input cmd, output resp))     benchmark;

connect from_server to benchmark.cmd;
connect to_server   to benchmark.resp;

use "d3bench.lku" for benchmark;
place benchmark on master;

place from_server on host;
place to_server on host;

