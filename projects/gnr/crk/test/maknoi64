	$(CC) $(CFLGS) -o bitortst $(DIR)/bitortst.o $(OBJ)/bitor.o \
		-L$(LIB) -lcrk

$(DIR)/bitortst.o:	bitortst.c $(INC)/sysdef.h $(INC)/bapkg.h \
		$(INC)/rkarith.h $(INC)/swap.h

