# LINUX MAKE file for ROCKS tests
# Version for testing code without HAS_I64 arithmetic
#
# V1A, 01/12/09, GNR - New version based on maklinux file

# Define directory where ROCKS library is stored
DIR := noi64

# Define directory where ROCKS library is stored
LIB := $(HOME)/lib/$(DIR)

# Define directory where include files are stored
INC := $(HOME)/include

# Define directory where crk object files are stored
# (Can be used to test a new version instead of	version	in LIB)
OBJ := $(HOME)/src/crk/$(DIR)

.SUFFIXES:
.SUFFIXES: .c .s .o

# Compilation macros for LINUX
CC := gcc
CFLGS := -g -Wunused
DEFS := PCLINUX _ISOC99_SOURCE NO_I64

#Compile for Linux
$(DIR)/%.o:	%.c makefile
	$(CC) $(CFLGS) $(DEFS:%=-D%) -I$(INC) -c $<
	mv $*.o	$(DIR)

# Targets for making various test programs
# (Note: Each invocation of rocks.h brings in rockv.h and rfdef.h)

algntest:	$(DIR)/algntest.o
	$(CC) $(CFLGS) -o algntest $(DIR)/algntest.o

$(DIR)/algntest.o:	algntest.c

bcditest:	$(DIR)/bcditest.o $(OBJ)/bcdin.o $(LIB)/libcrk.a
	$(CC) $(CFLGS) -o bcditest $(DIR)/bcditest.o $(OBJ)/bcdin.o \
		-L$(LIB) -lcrk -lm

$(DIR)/bcditest.o:	bcditest.c $(INC)/sysdef.h $(INC)/rkxtra.h

bcdotest:	$(DIR)/bcdotest.o $(LIB)/libcrk.a
	$(CC) $(CFLGS) -o bcdotest $(DIR)/bcdotest.o -L$(LIB) -lcrk -lm

$(DIR)/bcdotest.o:	bcdotest.c $(INC)/sysdef.h $(INC)/rocks.h $(INC)/rockv.h \
		$(INC)/rfdef.h $(INC)/rkxtra.h

